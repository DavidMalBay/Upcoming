function (err) {
      if (err) {
        return console.log(err);
      };
  
      console.log("The file was saved!");
      $(".table").append("<tr class='note' ><td>" + String(document.querySelector(".TextEditorTitle").value) + "<td class='text-right'>"+ "Today" + "</td>"  +"</td></tr>");

      $(".save").css({
        "color": "green"
      });
  
      $(".save").css({
        "color": "black"
      });
  
    }